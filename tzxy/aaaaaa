    [table] => `shop_cms_article` AS `cms_article`,`shop_cms_article_class` AS `cms_article_class`
    [field] => *
    [join] => Array
        (
            [0] => LEFT JOIN
        )

    [on] => cms_article.article_class_id = cms_article_class.class_id
    [where] => Array
        (
            [article_commend_flag] => 1
        )

    [order] => article_id desc
    [limit] => 9
)
